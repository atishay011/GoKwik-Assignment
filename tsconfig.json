{
    "compilerOptions": {
      "target": "es5",
      "lib": ["dom", "es2015"],
      "allowJs": true,
      "jsx": "react",
      "module": "commonjs",
      "moduleResolution": "node",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "outDir": "./dist"
    },
    "include": ["src"],
    "exclude": ["node_modules", "dist"]
  }